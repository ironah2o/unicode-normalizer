{
  "name": "unicode-normalizer",
  "main": "./lib/main",
  "version": "0.2.1",
  "description": "Unicode Normalizer for Atom",
  "keywords": [
    "Unicode equivalence",
    "normalize",
    "NFC",
    "NFD"
  ],
  "activationCommands": {
    "atom-text-editor": [
      "unicode-normalizer:convert-to-NFC",
      "unicode-normalizer:convert-to-NFKC",
      "unicode-normalizer:convert-to-NFD",
      "unicode-normalizer:convert-to-NFKD"
    ]
  },
  "repository": "https://github.com/ironah2o/unicode-normalizer",
  "license": "MIT",
  "engines": {
    "atom": ">=1.4.0 <2.0.0"
  },
  "dependencies": {
    "unorm": "1.4.1"
  }
}
